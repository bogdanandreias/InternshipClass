@{
    ViewData["Title"] = "Information about our classroom";
}

@section scripts {
    <script type="text/javascript" src="~/js/addNewcomer.js"></script>
}﻿
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Internship class</title>

</head>
<body>
    <h1>Internship Landpage</h1>
    <p>Here is our classroom</p>

    <ul id="list">
        @for (var i = 0; i < Model.Members.Count; i++)
        {
            var member = Model.Members[i];

            <li class="member">
                @member
            </li>
}
    </ul>

    <input id="newcomer" title="Name" type="text" />
    <button id="add">Add new commer</button>
    <p></p><img src="~/img/GoodCodeRules.png" width="200" />

</body>
</html>
